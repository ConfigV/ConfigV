# MySQL Configuration
# Generated by puppet - do not change by hand, your changes will be lost.

[mysqld]
datadir=/var/lib/mysql
user=mysql
skip-name-resolve
innodb_flush_method = O_DIRECT

<% mysql_editable_config.each do |config_key| -%>
<% if mysql_config.has_key?( config_key ) %>
<%= config_key %> = <%= mysql_config[ config_key ] %>
<% end -%>
<% end -%>

[mysqld_safe]
err-log=/var/log/mysqld.log
pid-file=/var/run/mysqld/mysqld.pid
