[client]
host= .
port= 3306
socket= "MySQL"

[mysql]
no-auto-rehash


[mysqld]

max_allowed_packet = 800M
thread_concurrency = 8
skip-external-locking
port=3306
basedir="c:/zpanel/bin/mysql/"
datadir="e:/hafez/bin/mysql/mysql5.5.24/data/"
character-set-server=utf8
default-storage-engine=MYISAM
max_connections=200
query_cache_size = 128M
table_cache=256
tmp_table_size=256M
thread_cache_size=8
myisam_max_sort_file_size=32G
myisam_sort_buffer_size=205M
key_buffer_size = 384M
read_buffer_size = 2M
read_rnd_buffer_size = 8M
sort_buffer_size = 2M




innodb_fast_shutdown=0
innodb_additional_mem_pool_size = 16M
innodb_log_buffer_size = 8M
innodb_buffer_pool_size=1800MB
innodb_log_file_size = 256M
innodb_thread_concurrency = 16
innodb_lock_wait_timeout = 120
innodb_file_per_table 

innodb_doublewrite = 0
#log-bin = 0
innodb_support_xa = 0
innodb_flush_log_at_trx_commit = 0


tmpdir= "e:/hafez/bin/mysql/mysql5.5.24/tmp/"

enable-named-pipe
skip-federated
server-id = 1

default-time-zone   = "SYSTEM"
log_error           = "c:/zpanel/logs/mysql/mysql.err"
pid_file            = "mysql.pid"
general_log         = 0
general_log_file    = "c:/zpanel/logs/mysql/mysql.log"
slow_query_log      = 0
slow_query_log_file = "c:/zpanel/logs/mysql/mysql-slow.log"


[myisamchk]
key_buffer_size = 1024M
sort_buffer_size = 1024M
read_buffer = 8M
write_buffer = 8M

[mysqldump]
quick
max_allowed_packet = 16M

[mysqlhotcopy]
interactive-timeout
