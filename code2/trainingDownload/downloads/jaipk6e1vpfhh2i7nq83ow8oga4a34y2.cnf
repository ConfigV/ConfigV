[client]
port            = 3306
socket          = /data/mysql/mysql.sock
host            = 127.0.0.1

[mysqld]
port = 3306
server_id = 1
socket = /data/mysql/mysql.sock
basedir = /data/mysql
datadir = /data/mysql/data

innodb_buffer_pool_size = 128M
join_buffer_size = 128M
sort_buffer_size = 2M
read_rnd_buffer_size = 2M 

sql_mode=NO_ENGINE_SUBSTITUTION,STRICT_TRANS_TABLES