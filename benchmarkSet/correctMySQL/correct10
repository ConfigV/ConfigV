[client]
port                           = 3306
socket                         = /var/run/mysql-foo/mysqld.sock

[mysqld]
datadir=/mysql/mydatabase
socket=/mysql/mydatabase/mysql.sock
port=3306
basedir="E:/Program Files/MySQL/MySQL Server 5.5/"
datadir="E:/Program Files/MySQL/MySQL Server 5.5/data/"
socket=/tmp/mysql.sock
character-set-server=latin1
default-storage-engine=INNODB
connection_buffer=64
max_connections=12
table_cache=512
open-files-limit=4096
tmp_table_size=32M
thread_cache_size=16

     
[mysql.server]
user=mysql
basedir=/mysql/mydatabase

[safe_mysqld]
err-log=/var/log/mysqld.log
pid-file=/var/run/mysqld/mysqld.pid
