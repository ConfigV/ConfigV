[client]
user = root
password = "root"

[mysql]
#pager = less -SF
pager = less -SFX

[mysqld]
general-log = TRUE
general-log-file = /usr/local/etc/mysql/general-log-file.log
log-bin = /usr/local/etc/mysql/log-bin.log
log-error = /usr/local/etc/mysql/log-error.log
slow-query-log = TRUE
slow-query-log-file = /usr/local/etc/mysql/slow-query-log-file.log
